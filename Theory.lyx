\begin_layout Plain Layout
\begin_inset Formula 
\[
=\frac{1}{2\mu_{eff}D}\left[e^{-\mu_{eff}\left|z-z_{0}\right|}-e^{-\mu_{eff}\left|z+z_{0}+2z_{b}\right|}\right]
\]

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard
Let us find the value of 
\begin_inset Formula $z$
\end_inset

 where we get the maximum:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{d}{dz}U_{1d}=\frac{d}{dz}\frac{1}{2\mu_{eff}D}\left[e^{-\mu_{eff}\left|z-z_{0}\right|}-e^{-\mu_{eff}\left|z+z_{0}+2z_{b}\right|}\right]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{1}{2\mu_{eff}D}\left[\frac{d}{dz}e^{-\mu_{eff}\left|z-z_{0}\right|}-\frac{d}{dz}e^{-\mu_{eff}\left|z+z_{0}+2z_{b}\right|}\right]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
=\frac{1}{2\mu_{eff}D}\left[-\mu_{eff}\frac{z-z_{0}}{\left|z-z_{0}\right|}e^{-\mu_{eff}\left|z-z_{0}\right|}+\mu_{eff}\frac{z+z_{0}+2z_{b}}{\left|z+z_{0}+2z_{b}\right|}e^{-\mu_{eff}\left|z+z_{0}+2z_{b}\right|}\right]
\]

\end_inset


\end_layout

\begin_layout Standard
