\begin_layout Subsection*
Diffusion approximation
\end_layout

\begin_layout Standard
The 3D solution using the diffusion approximation is:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
U=\frac{1}{4\pi D}\left[\frac{e^{-\mu_{eff}\sqrt{\left(z-z_{0}\right)^{2}+\rho^{2}}}}{\sqrt{\left(z-z_{0}\right)^{2}+\rho^{2}}}-\frac{e^{-\mu_{eff}\sqrt{\left(z+z_{0}+2z_{b}\right)^{2}+\rho^{2}}}}{\sqrt{\left(z+z_{0}+2z_{b}\right)^{2}+\rho^{2}}}\right]
\]

\end_inset


\end_layout

\begin_layout Standard
If we illuminate a half space, we will integrate on this:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
U_{1D}=\intop_{0}^{\infty}\intop_{0}^{2\pi}U\rho d\rho d\theta
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
=\intop_{0}^{\infty}\intop_{0}^{2\pi}\frac{1}{4\pi D}\left[\frac{e^{-\mu_{eff}\sqrt{\left(z-z_{0}\right)^{2}+\rho^{2}}}}{\sqrt{\left(z-z_{0}\right)^{2}+\rho^{2}}}-\frac{e^{-\mu_{eff}\sqrt{\left(z+z_{0}+2z_{b}\right)^{2}+\rho^{2}}}}{\sqrt{\left(z+z_{0}+2z_{b}\right)^{2}+\rho^{2}}}\right]\rho d\rho d\theta
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
=\frac{1}{4\pi D}\left[\intop_{0}^{\infty}\intop_{0}^{2\pi}\frac{e^{-\mu_{eff}\sqrt{\left(z-z_{0}\right)^{2}+\rho^{2}}}}{\sqrt{\left(z-z_{0}\right)^{2}+\rho^{2}}}\rho d\rho d\theta-\intop_{0}^{\infty}\intop_{0}^{2\pi}\frac{e^{-\mu_{eff}\sqrt{\left(z+z_{0}+2z_{b}\right)^{2}+\rho^{2}}}}{\sqrt{\left(z+z_{0}+2z_{b}\right)^{2}+\rho^{2}}}\rho d\rho d\theta\right]
\]

\end_inset


\end_layout

\begin_layout Standard
Let us solve:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\intop_{0}^{\infty}\intop_{0}^{2\pi}\frac{e^{-\mu\sqrt{a^{2}+\rho^{2}}}}{\sqrt{a^{2}+\rho^{2}}}\rho d\rho d\theta=2\pi\intop_{0}^{\infty}\frac{e^{-\mu\sqrt{a^{2}+\rho^{2}}}}{\sqrt{a^{2}+\rho^{2}}}\rho d\rho
\]

\end_inset


\begin_inset Formula 
\[
=2\pi\frac{e^{-\mu\left|a\right|}}{\mu}
\]

\end_inset


\end_layout

\begin_layout Standard
Therefore:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{1}{4\pi D}\left[\intop_{0}^{\infty}\intop_{0}^{2\pi}\frac{e^{-\mu_{eff}\sqrt{\left(z-z_{0}\right)^{2}+\rho^{2}}}}{\sqrt{\left(z-z_{0}\right)^{2}+\rho^{2}}}\rho d\rho d\theta-\intop_{0}^{\infty}\intop_{0}^{2\pi}\frac{e^{-\mu_{eff}\sqrt{\left(z+z_{0}+2z_{b}\right)^{2}+\rho^{2}}}}{\sqrt{\left(z+z_{0}+2z_{b}\right)^{2}+\rho^{2}}}\rho d\rho d\theta\right]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Box Boxed
